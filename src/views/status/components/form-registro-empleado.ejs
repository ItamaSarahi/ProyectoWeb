<form class="col s12" form id="formStatus" action="/catalogo/empleado/registrarEmpleados" method="post">
   

    <body class="cyan darken-1">
        <br>
     
        <div class="container grey lighten-2"  style="width: 850px; margin-left: 30%;  padding: 10px; ">
        
            <h3 style="text-align: center; font-family: 'Franklin Gothic Medium'">CREATIVE IDEAS </h3>
            <h6 style="text-align: center; font-style:italic">Registro de empleados</h6>
    
            <br>
    
    
    
            <div class="row" style=" padding: 30px; ">
                
                <!--Division de materialize de columnas-->
                <form class="col s12">
    
    
                    <div class="row">
                        <div class="input-field col s4">
                            <!---Esto es la imagen: account circle (materialize)-->
                            <i class="material-icons prefix">account_circle</i>
                            <!--Input tipo text -->
                            <input type="text" name="nombre_E" id="nombre_E">
                            <!--Etiqueta de texto-->
                            <label for="nombre_E">Nombre</label>
                        </div>
                        
    
                        
                        <div class="input-field col s4">
                            <input type="text" name="apellidoPE" id="apellidoPE">
                            <label for="apellidoPaterno">Apellido paterno</label>
                        </div>
    
                        <div class="input-field col s4">
                            <input type="text" name="apellidoME" id="apellidoME">
                            <label for="apellidoMaterno">Apellido materno</label>
                        </div>
                   
                    </div>
    
    
    
                    
    
                    <!--División de materialize tipo input field-->
                    <div class="input-field">
                        <!--Icóno-->
                        <i class="material-icons prefix">email</i>
                        <!--Input de tipo texto-->
                        <input type="text" name="email" id="email">
                        <label for="email">Email</label>
                    </div>
    
    
                     <!--División de materialize tipo input field-->
                     <div class="input-field">
                        <!--Icóno-->
                        <i class="material-icons prefix">grade</i>
                        <!--Input de tipo texto-->
                        <input type="text" name="nivelEstudio" id="nivelEstudio">
                        <label for=">nivelEstudio">Nivel de estudios</label>
                    </div>
    
    
    
                    <!--División de materialize tipo input field-->
                    <div class="input-field">
                        <!--Icóno-->
                        <i class="material-icons prefix">account_box</i>
                        <!--Input de tipo texto-->
                        <input type="text" name="usuario" id="usuario">
                        <label for="usuario">Usuario</label>
                    </div>
    
    
    
                    <!--División de materialize tipo input field-->
                    <div class="input-field">
                        <!--Icóno (password)-->
                        <i class="material-icons prefix">lock</i>
                        <!--Input de tipo contraseña-->
                        <input type="password" name="password" id="password">
                        <label for="password">Contraseña</label>
                    </div>
    
    
                     <!--Este es el botón-->
                     <button type="submit" class="btn waves-effect cyan darken-1" style="margin-left: 320px">
                        <!--Este es el icono del boton (o sea, la flechita)-->
                        <i class="material-icons right">check</i> Registrar
            
                    </button>
    
                    
    
                   
                </form>
    
            </div>
    
    
        </div> <!-- CONTAINER -->
        <br>

        <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
            
        <% if(typeof alert!="undefined" ){%>
            <script>
                Swal.fire({
                    title: '<%= alertTitle %>',
                    text: '<%= alertMessage %>',
                    icon: '<%= alertIcon%>',
                })
            </script>
            <%}%>
    
        
       
    </body>
    
    </form>